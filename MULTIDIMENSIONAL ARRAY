int addarray(int a1[][],int a2[][]);
int addarray(int a1[][],int a2[][])
{
    int res[50][50];
    for(int i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
           res[i][j]=a1[i][j]+a2[i][j];
           printf("%d ",res[i][j]);
        }
    }
}
int main()
{
    int a[2][2]={{1,2},
                  {3,4}};
    int b[2][2]={{1,2},
                 {3,4}};
    addarray(a,b);
    return 0;
}
