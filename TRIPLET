#include<stdio.h>
void main()
{

    int a,i,j,k;
    printf("enter any number");
    scanf("%d",&a);
    for(int i=5;i<a;i++)
    {
        for(int j=4;j<i;j++)
        {
            for(int k=3;k<j;k++)
            {

                if(j*j+k*k==i*i)
                    printf("\n%d%d%d",k,j,i);

            }
        }
    }
}
