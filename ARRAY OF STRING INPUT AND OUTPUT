#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
 char a[10][10];
 char b;
 int i;
 printf("enter no of names");
 scanf("%d",b);
 printf("enter all names");
 for(i=0;i<b;i++)
  {
     scanf("%s",a[i]);
  }
  printf("the length are");
  for(int i=0;i<b;i++)
  {
   printf("\n no %d . is",i+1,a[i]);
   }
}
